# editorconfig.org
root = true

[*]
indent_style = space
indent_line = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.html]
indent_style = space
indent_line = 2

[*.{scss,css,sass}]
indent_style = space
indent_line = 2

[*.md]
trim_trailing_whitespace = false
